/* a) 1, 3, 5, 7, ___ próximo número 9, porque está imprimindo somente os números ímpares*/

import java.util.Scanner;
 
public class Main {
	public static void main(String[] args) {
	
	 Scanner sc = new Scanner(System.in);
	 
	 int next_impar, total_numero, numero; 
	 
	 System.out.print("Quantos números vão ter na sequência: ");
	 total_numero = sc.nextInt();
	 
	 for (int i = 1; i < total_numero*2; i++) {
	   if (i % 2 != 0) {
	    System.out.print(i + " ");
	    }
	 }
	
	sc.close();
	}
}
